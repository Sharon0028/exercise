#include <iostream>
using namespace std;

class Counter{
private:
    int value;
public:
    Counter(){
        value=0;
    }
    void increase(){
        value+=1;
    }
    int getValue(){
        return value;
    }
};
int main(){
    Counter c;
    c.increase();
    c.increase();
    cout<<c.getValue();
    return 0;
}
