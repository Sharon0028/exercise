#include <iostream>
using namespace std;
class Car{
protected:
    int speed;
public:
    Car(){
        speed=0;
    }
    void accelerate(){
        speed+=10;
    }
    int getSpeed(){
        return speed;
    }
};
class ElectricCar:public Car{
public:
    void boost(){
        speed+=20;
    }
};
int main(){
    ElectricCar e;
    e.accelerate();
    e.boost();
    cout<<e.getSpeed();
    return 0;
}
